<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.come.restaurants">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".open.restaurant.ui.OpenRestaurantActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".order.list.ui.OrderListActivity"/>
        <activity android:name=".order.detail.OrderDetailActivity"/>
        <activity android:name=".printer.pairing.ui.BtPairingActivity">
        </activity>
    </application>

</manifest>